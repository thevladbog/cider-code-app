{
  "name": "bottle-code-app",
  "productName": "Bottle [CODE] App",
  "version": "1.0.0-beta.2",
  "description": "Система агрегации в транспортные короба",
  "main": ".webpack/main",
  "scripts": {
    "start": "npx electron-forge start",
    "start:dev": "cross-env NODE_ENV=development npx electron-forge start",
    "package": "npx electron-forge package",
    "make": "npx electron-forge make",
    "make:win": "set TARGET_PLATFORM=win32 && npx electron-forge make --platform=win32",
    "make:linux": "set TARGET_PLATFORM=linux && npx electron-forge make --platform=linux",
    "make:mac": "set TARGET_PLATFORM=darwin && npx electron-forge make --platform=darwin",
    "publish": "npx electron-forge publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npm run prettier:fix && npm run lint:fix",
    "rebuild": "electron-rebuild",
    "semantic-release": "semantic-release",
    "generate-api-client": "curl -k -o ./src/app/api/openapi.yaml https://api.test.in.bottlecode.app:3035/api/yaml && openapi --input ./src/app/api/openapi.yaml --output ./src/app/api/generated --client axios --useOptions --useUnionTypes"
  },
  "keywords": [],
  "author": {
    "name": "Vladislav Bogatyrev",
    "email": "vladislav.bogatyrev@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.27.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/react-dom": "^19.1.6",
    "@vercel/webpack-asset-relocator-loader": "^1.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "electron": "36.2.1",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "node-loader": "^2.1.0",
    "openapi-typescript-codegen": "^0.29.0",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "semantic-release": "^22.0.12",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "webpack": "^5.99.8"
  },
  "dependencies": {
    "@gravity-ui/date-components": "^3.1.1",
    "@gravity-ui/date-utils": "^2.5.6",
    "@gravity-ui/icons": "^2.13.0",
    "@gravity-ui/uikit": "^7.12.0",
    "@tanstack/react-query": "^5.76.2",
    "axios": "^1.9.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "jsbarcode": "^3.11.6",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^7.6.0",
    "serialport": "^10.5.0",
    "usb": "^2.15.0",
    "zustand": "^5.0.5"
  }
}
