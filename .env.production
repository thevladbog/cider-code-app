# Конфигурация для production окружения
NODE_ENV=production
VITE_APP_ENV=production

# Yandex Cloud настройки для production
YANDEX_CLOUD_LOGGING_ENABLED=true
YANDEX_USE_SDK=true

# Production логирование (обычно используется Service Account Key вместо токенов)
YANDEX_FOLDER_ID=your-production-folder-id
YANDEX_LOG_GROUP_ID=your-production-log-group-id
APP_INSTANCE_ID=bottle-code-app-prod-instance

# Service Account Key должен быть настроен для production
# YANDEX_SERVICE_ACCOUNT_KEY=...
